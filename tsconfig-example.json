{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./example",
    "outDir": "./dist-example",
    "noEmitOnError": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true
  },
  "include": [
    "example/**/*"
  ]
}
